  },
  "homepage": "https://docs.chemchat.com/sdk/javascript",
  "dependencies": {
    "socket.io-client": "^4.8.1",
    "axios": "^1.6.2",
    "eventemitter3": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "rollup": "^4.6.1"
  },
  "engines": {
    "node": ">=14.0.0"
